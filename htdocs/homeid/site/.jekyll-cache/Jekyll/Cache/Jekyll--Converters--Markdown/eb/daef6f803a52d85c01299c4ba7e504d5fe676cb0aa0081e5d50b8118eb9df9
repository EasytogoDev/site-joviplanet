            <a class="nav-link active" data-toggle="tab" href="#result-36" role="tab" aria-controls="result-36" aria-selected="true">Result</a>
        </li>
        <li class="nav-item" role="presentation">
            <a class="nav-link" data-toggle="tab" href="#html-36" role="tab" aria-controls="html-36" aria-selected="false">HTML Code</a>
        </li>
    </ul>

